{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": 1,
    "no-console": 1
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "globals": {
    "window": true,
    "THREE": true,
    "GPUComputationRenderer": true
  }
}
